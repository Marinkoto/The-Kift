  dialogueLines:
  - speaker: ???
    text: It's...you?
    options:
    - optionText: Ohh so that's what happened!
      nextLineIdentifier: 1
    - optionText: I see what you have done!
      nextLineIdentifier: 2
  - speaker: Roggi
    text: Good meeting you old friend!
    options:
    - optionText: Pleasure meeting you again!
      nextLineIdentifier: 3
  - speaker: Roggi
    text: So how did you got here?
    options:
